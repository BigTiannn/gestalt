{
  "name": "pinterest-gestalt",
  "version": "0.0.1",
  "description": "Pinterest web components written in React.",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "rm -f dist/main.js",
    "clobber": "rm -rf dist",
    "devserver": "webpack-dev-server --hot --inline --progress --colors --config webpack.config.docs.js",
    "lint:eslint": "./node_modules/eslint/bin/eslint.js . --ext js",
    "lint:stylelint": "./node_modules/stylelint/dist/cli.js src/**/*.css",
    "lint": "npm run lint:eslint ; npm run lint:stylelint",
    "start": "npm run devserver",
    "test": "./node_modules/jest-cli/bin/jest.js ."
  },
  "author": "Pinterest",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^10.0.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "devcards": "chrislloyd/devcards",
    "eslint": "^2.5.3",
    "eslint-config-pinterest": "^17.0.0",
    "eslint-plugin-pinterest": "^3.0.0",
    "eslint-plugin-react": "^4.2.3",
    "jest-cli": "^0.10.0",
    "open-browser-webpack-plugin": "0.0.2",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "stylelint": "^5.1.0",
    "stylelint-config-standard": "^4.0.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.0"
  },
  "peerDependencies": {
    "babel-loader": "^6.2.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "style-loader": "^0.13.0",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.8.2",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-import": "^8.1.0",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "scriptPreprocessor": "<rootDir>/jestPreprocessor.js"
  }
}
